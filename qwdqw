#include <bits/stdc++.h>
using namespace std;

#define IOS                \
  ios::sync_with_stdio(0); \
  cin.tie(0);              \
  cout.tie(0);

int main() {
  IOS

  int n;
  cin >> n;

  vector<int> t(n);
  for(int i = 0; i < n; i++) {
    cin >> t[i];
  }

  int counter = 0;
  for(int i = 0; i < n; i++) {
    if(t[i] < 0) {
      counter++;
    }
  }

  cout << counter << endl;

  return 0;
}
